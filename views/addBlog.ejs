<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('partials/head') %>
    <title>Blog Page</title>
</head>

<body>


  <%- include('partials/nav') %>
    <div class="container mt-3">
      <form action="/blog" method="post" enctype="multipart/form-data">
        <!--  -->
        <div class="mt-3">
          <label for="file">Cover Img</label>
          <input type="file" name="file" id="file" class="form-control" placeholder="Upload File">
        </div>



        <!--  -->
        <div class="mt-3">
          <label for="title">Title</label>
          <input type="text" name="title" id="title" class="form-control" placeholder="Title" aria-describedby="title">
        </div>
        <!--  -->
        <div class="mt-3">
          <label for="body">Content</label>
          <textarea class="form-control" rows="10" placeholder="Content of Blog" id="body" name="body"></textarea>
        </div>
        <div class="mt-3">
          <button type="submit" class="btn btn-primary">Submit</button>
        </div>

      </form>
    </div>

    <%- include('partials/scripts') %>
</body>

</html>